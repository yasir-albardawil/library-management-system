<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="420.0" prefWidth="145.0" stylesheets="@theme.css" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="net.buammar.controller.ToolBarController">
   <children>
      <JFXButton contentDisplay="TOP" mnemonicParsing="false" onAction="#handleAddMember" prefHeight="400.0" prefWidth="150.0" styleClass="toolbar-button" text="Add member" />
      <JFXButton contentDisplay="TOP" lineSpacing="1.0" mnemonicParsing="false" onAction="#handleAddBook" prefHeight="400.0" prefWidth="150.0" styleClass="toolbar-button" text="Add book" />
      <JFXButton contentDisplay="TOP" lineSpacing="2.0" mnemonicParsing="false" onAction="#handleMemberTable" prefHeight="400.0" prefWidth="150.0" styleClass="toolbar-button" text="View members" />
      <JFXButton contentDisplay="TOP" lineSpacing="3.0" mnemonicParsing="false" onAction="#handleBookTable" prefHeight="400.0" prefWidth="150.0" styleClass="toolbar-button" text="View books" />
      <JFXButton contentDisplay="TOP" lineSpacing="4.0" mnemonicParsing="false" onAction="#handleSettings" prefHeight="400.0" prefWidth="150.0" styleClass="toolbar-button" text="Settings" />
   </children>
</VBox>
